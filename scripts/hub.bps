	gamerule(doDaylightCycle, false)
	gamerule(doWeatherCycle, false)
	/gamemode adventure @a
	/time set 6000
	/setblock 0 61 1006 minecraft:diamond_block
	/setblock 0 61 1005 minecraft:stone_button[face=wall,facing=north,powered=false]
	/setblock -4 61 1005 minecraft:gold_block
	/setblock -4 61 1004 minecraft:stone_button[face=wall,facing=north,powered=false]
	/setblock -4 62 1005 minecraft:player_head[rotation=0]{SkullOwner:{Id:[I;356124168,1183928132,-2129165978,-1124639305],Properties:{textures:[{Signature:"",Value:""}]},Name:"MHF_YouTube"}}
