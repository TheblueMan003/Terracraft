def ticking hubmain(){
	if (block(-4 61 1004 minecraft:stone_button[face=wall,facing=north,powered=true])){
		/setblock -4 61 1004 minecraft:stone_button[face=wall,facing=north,powered=false]
		tellraw(@a,("Map Made By ",yellow),("TheblueMan003",gold))
		tellraw(@a,("   You",white,"https://www.youtube.com/channel/UCIcGrc1e1Y4UNSqJCfyufkA"),("tube",red,"https://www.youtube.com/channel/UCIcGrc1e1Y4UNSqJCfyufkA"))
		tellraw(@a,("   PlanetMinecraft  ",aqua,"https://www.planetminecraft.com/member/theblueman003/"))
	}
	if (block(0 61 1005 minecraft:stone_button[face=wall,facing=north,powered=true])){
		/setblock 0 61 1005 minecraft:air
		generation.startwithoutpreview()
	}
	if (block(4 61 1004 minecraft:stone_button[face=wall,facing=north,powered=true])){
		/setblock 4 61 1004 minecraft:stone_button[face=wall,facing=north,powered=false]
		tellraw(@a,("Source Code:"))
		tellraw(@a,("   https://github.com/TheblueMan003/Terracraft","https://github.com/TheblueMan003/Terracraft"))
		tellraw(@a,("Some of My Other Maps:",yellow))
		tellraw(@a,("   Super Steve Bros",aqua,"https://www.planetminecraft.com/project/super-steve-bros-4833363/"))
		tellraw(@a,("   Rewind Time",aqua,"https://www.planetminecraft.com/project/rewind-time/"))
		tellraw(@a,("   Blockception Parkour 4",aqua,"https://www.planetminecraft.com/project/blockception-parkour-4/"))
		tellraw(@a,("   The Legend of Alex",aqua,"https://www.planetminecraft.com/project/the-legend-of-alex-a-crafted-adventure/"))
	}
}
def playerInit(){
	with(@s[tag=!inGame]){
		/tp @s 0 61 1000
	}
}